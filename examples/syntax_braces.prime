[return<int> effects(io_out)]
main() {
  [i32] limit{3i32}
  [array<i32>] values{array<i32>{1i32, 2i32, 3i32, 4i32}}
  [i32] first{at(values, 0i32)}
  [i32] second{at(values, 1i32)}
  [i32] sum{plus(first, second)}
  [i32] total{plus(sum, values.count())}

  print_line(total)

  if(less_than(total, limit)) {
    print_line("below limit"utf8)
  } else {
    print_line("at or above"utf8)
  }

  return(total)
}
