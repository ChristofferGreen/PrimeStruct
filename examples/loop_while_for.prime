[return<int>]
main() {
  [i32 mut] total{0i32}
  [i32 mut] i{0i32}

  loop(3i32) {
    assign(total, plus(total, 1i32))
  }
  while(less_than(i, 4i32)) {
    assign(total, plus(total, i))
    assign(i, plus(i, 1i32))
  }

  for([i32 mut] j{0i32}; less_than(j, 3i32); assign(j, plus(j, 1i32))) {
    assign(total, plus(total, j))
  }

  return(total)
}
